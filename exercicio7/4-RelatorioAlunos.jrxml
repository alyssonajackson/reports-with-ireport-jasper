	<queryString>
		<![CDATA[SELECT

P.nome,
P.cpf,
A.data_aniversario,
A.matricula,
D.id,
D.curso,
D.nome AS nome_cur

FROM TB_ALUNO A

LEFT JOIN TB_DISCIPLINA_TB_ALUNO DA on DA.alunos_id=A.id
LEFT JOIN TB_DISCIPLINA D on D.id=DA.TB_DISCIPLINA_ID
LEFT JOIN TB_PESSOA P on P.id=A.id order by P.nome asc]]>
	</queryString>
